<header class="flex flex-col lg:flex-row justify-between items-center lg:max-w-7xl md:max-w-5xl mx-auto bg-gray-100 p-5">
  <div class="flex w-full lg:w-auto items-center justify-between">
    <a href="/" class="text-lg">
      <span class="font-bold text-slate-800 flex items-center">
        <svg width="1em" height="1em" viewBox="0 0 24 24" class="black-white w-5 h-5 mr-1" data-icon="bx:bxs-rocket">
          <symbol id="ai:bx:bxs-rocket">
            <path d="M15.78 15.84S18.64 13 19.61 12c3.07-3 1.54-9.18 1.54-9.18S15 1.29 12 4.36C9.66 6.64 8.14 8.22 8.14 8.22S4.3 7.42 2 9.72L14.25 22c2.3-2.33 1.53-6.16 1.53-6.16zm-1.5-9a2 2 0 0 1 2.83 0a2 2 0 1 1-2.83 0zM3 21a7.81 7.81 0 0 0 5-2l-3-3c-2 1-2 5-2 5z" fill="currentColor"></path>
          </symbol>
          <use xlink:href="#ai:bx:bxs-rocket"></use>
        </svg>Factu<span class="text-slate-500 ml-1">Zen</span>
      </span>
    </a>
    <div class="block lg:hidden">
      <button id="astronav-menu" aria-label="Toggle Menu">
        <svg fill="currentColor" class="w-4 h-4 text-gray-800" width="24" height="24" viewBox="0 0 24 24" xmlns="https://www.w3.org/2000/svg">
          <title>Toggle Menu</title>
          <path class="astronav-close-icon astronav-toggle hidden" fill-rule="evenodd" clip-rule="evenodd" d="M18.278 16.864a1 1 0 01-1.414 1.414l-4.829-4.828-4.828 4.828a1 1 0 01-1.414-1.414l4.828-4.829-4.828-4.828a1 1 0 011.414-1.414l4.829 4.828 4.828-4.828a1 1 0 111.414 1.414l-4.828 4.829 4.828 4.828z"></path>
          <path class="astronav-open-icon astronav-toggle" fill-rule="evenodd" d="M4 5h16a1 1 0 010 2H4a1 1 0 110-2zm0 6h16a1 1 0 010 2H4a1 1 0 010-2zm0 6h16a1 1 0 010 2H4a1 1 0 010-2z"></path>
        </svg>
      </button>
    </div>
  </div>
  <nav id="astronav-items" class="astronav-toggle hidden w-full lg:w-auto mt-2 lg:flex lg:mt-0">
    <ul class="flex flex-col lg:flex-row lg:gap-3">
      <li>
        <a href="/" class="flex lg:px-3 py-2 items-center text-gray-600 hover:text-gray-900">
          <span>Inicio</span>
        </a>
      </li>
      <li>
        <a href="/Pricing" class="flex lg:px-3 py-2 items-center text-gray-600 hover:text-gray-900">
          <span>Precios</span>
        </a>
      </li>
      <li>
        <a href="/contact" class="flex lg:px-3 py-2 items-center text-gray-600 hover:text-gray-900">
          <span>Contacto</span>
        </a>
      </li>
    </ul>
  </nav>
</header>

<script>
document.addEventListener('DOMContentLoaded', function () {
  const menuButton = document.getElementById('astronav-menu');
  const navMenu = document.getElementById('astronav-items');
  const closeIcon = document.querySelector('.astronav-close-icon');
  const openIcon = document.querySelector('.astronav-open-icon');

  // Verificar si los elementos existen antes de agregar el event listener
  if (menuButton && navMenu && closeIcon && openIcon) {
    menuButton.addEventListener('click', function () {
      navMenu.classList.toggle('hidden');
      closeIcon.classList.toggle('hidden');
      openIcon.classList.toggle('hidden');
    });
  } else {
    console.error('Uno o m√°s elementos no se encontraron en el DOM.');
  }
});
</script>